@model FastReport.Web.WebReport
@using NajmetAlraqee.Site.Services;
@using NajmetAlraqeeSite.Resources

@inject LocService SharedLocalizer
<br />
<br />
<div class="col-md-12">
    <div class="card">
        <br />
        <form asp-action="Index">
            <div class="raw">
                <div class="col-md-12">
                    <div class="form-group m-t-0">
                        <div class="form-line">
                            <input class="form-control" name="SearchString" style="font-family:Droid Arabic Kufi" placeholder="أسم الحساب" type="text">
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <button class="btn btn-raised btn-sm btn-success m-b-0 m-t-0" style="font-family:Droid Arabic Kufi;right:1080px" type="submit">@SharedLocalizer.GetLocalizedHtmlString("Search")</button>
                    </div>
                </div>
            </div>
        </form>
        <br />
    </div>
</div>
<br />
<div class="card">
    <br />
    <div class="row">
        <div class="col-md-12">
            @await Model.Render()
        </div>
        <br />
    </div>
</div>



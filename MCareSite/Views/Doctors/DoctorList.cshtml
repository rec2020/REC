@model NajmetAlraqee.Data.Entities.DoctorLanguage

<div class="row">
<div class="col-md-4">
    <form asp-action="Create" asp-Controller="DoctorLanguages">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        @Html.HiddenFor(x=>x.DoctorId)
        <div class="form-group">
            <label asp-for="LanguageId" class="control-label"></label>
            <select asp-for="LanguageId" class="form-control" asp-items="ViewBag.LanguageId"></select>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </form>
</div>
</div>